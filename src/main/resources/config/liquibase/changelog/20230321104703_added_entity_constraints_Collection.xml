<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Collection.
    -->
    <changeSet id="20230321104703-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="collection_id"
                                 baseTableName="rel_collection__books"
                                 constraintName="fk_rel_collection__books__collection_id"
